{
  "name": "abi-node",
  "version": "0.1.0-beta.3",
  "description": "ABI-first mock Ethereum RPC server for frontend development",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "abi-node": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "docs"
  ],
  "scripts": {
    "dev": "tsx --watch src/cli.ts",
    "build": "tsup",
    "test": "vitest",
    "start": "node dist/cli.js",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ethereum",
    "rpc",
    "mock",
    "abi",
    "web3",
    "blockchain",
    "testing",
    "viem",
    "wagmi",
    "frontend"
  ],
  "author": "Lancers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/0x3f-lancers/abi-node"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.25.0",
  "dependencies": {
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "fastify": "^5.6.2",
    "viem": "^2.43.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.0.4"
  }
}
